{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/app.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ],
    "functions": {
        "backend/app.py": {
            "runtime": "vercel-python@0.1.0"
        }
    }
}